{
  "dataRoles": [
    {
      "displayName": "Program Identifier",
      "description": "The program identifier (used as Y-axis label)",
      "kind": "GroupingOrMeasure",
      "name": "program_id"
    },
    {
      "displayName": "Program Category",
      "description": "Category for color grouping and legend",
      "kind": "GroupingOrMeasure",
      "name": "program_category"
    },
    {
      "displayName": "Start Date",
      "description": "Trial start date",
      "kind": "GroupingOrMeasure",
      "name": "start_date"
    },
    {
      "displayName": "End Date",
      "description": "Estimated trial end date",
      "kind": "GroupingOrMeasure",
      "name": "end_date"
    },
    {
      "displayName": "Mouse Over",
      "description": "Additional attributes shown on hover (optional, accepts multiple fields)",
      "kind": "GroupingOrMeasure",
      "name": "mouseover"
    },
    {
      "displayName": "Milestone",
      "description": "Milestone label text (optional, paired with Milestone Time)",
      "kind": "GroupingOrMeasure",
      "name": "milestone"
    },
    {
      "displayName": "Milestone Time",
      "description": "Date when the milestone occurred (optional, paired with Milestone)",
      "kind": "GroupingOrMeasure",
      "name": "milestone_time"
    }
  ],
  "dataViewMappings": [
    {
      "conditions": [
        {
          "program_id": {
            "max": 1
          },
          "program_category": {
            "max": 1
          },
          "start_date": {
            "max": 1
          },
          "end_date": {
            "max": 1
          },
          "mouseover": {
            "min": 0
          },
          "milestone": {
            "max": 1,
            "min": 0
          },
          "milestone_time": {
            "max": 1,
            "min": 0
          }
        }
      ],
      "scriptResult": {
        "dataInput": {
          "table": {
            "rows": {
              "select": [
                {
                  "for": {
                    "in": "program_id"
                  }
                },
                {
                  "for": {
                    "in": "program_category"
                  }
                },
                {
                  "for": {
                    "in": "start_date"
                  }
                },
                {
                  "for": {
                    "in": "end_date"
                  }
                },
                {
                  "for": {
                    "in": "mouseover"
                  }
                },
                {
                  "for": {
                    "in": "milestone"
                  }
                },
                {
                  "for": {
                    "in": "milestone_time"
                  }
                }
              ],
              "dataReductionAlgorithm": {
                "top": {}
              }
            }
          }
        },
        "script": {
          "scriptProviderDefault": "R",
          "scriptOutputType": "html",
          "source": {
            "objectName": "rcv_script",
            "propertyName": "source"
          },
          "provider": {
            "objectName": "rcv_script",
            "propertyName": "provider"
          }
        }
      }
    }
  ],
  "objects": {
    "rcv_script": {
      "properties": {
        "provider": {
          "type": {
            "text": true
          }
        },
        "source": {
          "type": {
            "scripting": {
              "source": true
            }
          }
        }
      }
    }
  },
  "suppressDefaultTitle": true,
  "privileges": [
    {
      "name": "WebAccess",
      "parameters": [
        "https://cdn.plot.ly/"
      ]
    }
  ]
}
