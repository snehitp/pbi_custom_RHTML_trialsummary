{
  "dataRoles": [
    {
      "displayName": "CAR Target",
      "description": "The CAR target name (e.g., CD19, CD22, GD2)",
      "kind": "GroupingOrMeasure",
      "name": "car_target"
    },
    {
      "displayName": "Cancer Type",
      "description": "The cancer type or indication",
      "kind": "GroupingOrMeasure",
      "name": "cancer_type"
    },
    {
      "displayName": "Start Date",
      "description": "Trial start date",
      "kind": "GroupingOrMeasure",
      "name": "start_date"
    },
    {
      "displayName": "End Date",
      "description": "Estimated trial end date",
      "kind": "GroupingOrMeasure",
      "name": "end_date"
    },
    {
      "displayName": "Mouse Over",
      "description": "Additional attributes shown on hover (optional, accepts multiple fields)",
      "kind": "GroupingOrMeasure",
      "name": "mouseover"
    }
  ],
  "dataViewMappings": [
    {
      "conditions": [
        {
          "car_target": {
            "max": 1
          },
          "cancer_type": {
            "max": 1
          },
          "start_date": {
            "max": 1
          },
          "end_date": {
            "max": 1
          },
          "mouseover": {
            "min": 0
          }
        }
      ],
      "scriptResult": {
        "dataInput": {
          "table": {
            "rows": {
              "select": [
                {
                  "for": {
                    "in": "car_target"
                  }
                },
                {
                  "for": {
                    "in": "cancer_type"
                  }
                },
                {
                  "for": {
                    "in": "start_date"
                  }
                },
                {
                  "for": {
                    "in": "end_date"
                  }
                },
                {
                  "for": {
                    "in": "mouseover"
                  }
                }
              ],
              "dataReductionAlgorithm": {
                "top": {}
              }
            }
          }
        },
        "script": {
          "scriptProviderDefault": "R",
          "scriptOutputType": "html",
          "source": {
            "objectName": "rcv_script",
            "propertyName": "source"
          },
          "provider": {
            "objectName": "rcv_script",
            "propertyName": "provider"
          }
        }
      }
    }
  ],
  "objects": {
    "rcv_script": {
      "properties": {
        "provider": {
          "type": {
            "text": true
          }
        },
        "source": {
          "type": {
            "scripting": {
              "source": true
            }
          }
        }
      }
    }
  },
  "suppressDefaultTitle": true,
  "privileges": [
    {
      "name": "WebAccess",
      "parameters": [
        "https://cdn.plot.ly/"
      ]
    }
  ]
}
